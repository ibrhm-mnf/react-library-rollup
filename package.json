{
  "name": "react-library-rollup",
  "version": "1.0.0",
  "description": "This is a react library bundled using rollup",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "sideEffects": false,
  "scripts": {
    "prepare": "rollup --config",
    "clean": "rimraf dist",
    "build": "npm run clean && rollup -c",
    "start": "npm run clean && rollup -c -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibrhm-mnf/react-library-rollup.git"
  },
  "keywords": [
    "react",
    "rollup"
  ],
  "author": "Ibrahim Manaf P",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ibrhm-mnf/react-library-rollup/issues"
  },
  "homepage": "https://github.com/ibrhm-mnf/react-library-rollup#readme",
  "peerDependencies": {
    "react": ">=16.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "core-js": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-standard": "^4.0.1",
    "react": "^16.13.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.7.6",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-terser": "^5.3.0"
  },
  "engines": {
    "node": ">=8"
  },
  "files": [
    "dist"
  ]
